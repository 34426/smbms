<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <!--日志输出-->
        <setting name="logImpl" value="STDOUT_LOGGING"/>
        <!--自动驼峰映射-->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <mappers>
        <mapper class="com.igeek.mapper.ProviderMapper"/>
        <mapper class="com.igeek.mapper.BillMapper"/>
        <mapper class="com.igeek.mapper.RoleMapper"/>
        <mapper class="com.igeek.mapper.UserMapper"/>
    </mappers>
</configuration>